{
  "lexicon": 1,
  "id": "ar.cabildoabierto.data.dataBlock",
  "defs": {
    "main": {
      "type": "record",
      "key": "tid",
      "record": {
        "type": "object",
        "required": ["dataset", "createdAt", "data", "format"],
        "properties": {
          "dataset": {
            "type": "string",
            "format": "at-uri"
          },
          "data": {
            "type": "blob"
          },
          "format": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
          },
          "createdAt": {
            "type": "string",
            "format": "datetime",
            "description": "Client-declared timestamp when this post was originally created."
          }
        }
      }
    }
  }
}