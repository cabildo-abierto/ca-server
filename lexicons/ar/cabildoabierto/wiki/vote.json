{
    "lexicon": 1,
    "id": "ar.cabildoabierto.wiki.vote",
    "defs": {
      "main": {
        "type": "record",
        "key": "tid",
        "record": {
          "type": "object",
          "required": ["subject", "createdAt"],
          "properties": {
            "subject": {
              "type": "ref",
              "ref": "com.atproto.repo.strongRef"
            },
            "createdAt": {
              "type": "string",
              "format": "datetime"
            },
            "value": {
              "type": "string",
              "description": "Valid values are 'accept' and 'reject'."
            },
            "message": {
              "type": "string",
              "maxGraphemes": 600,
              "maxLength": 6000
            },
            "labels": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }