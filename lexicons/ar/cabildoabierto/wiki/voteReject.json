{
    "lexicon": 1,
    "id": "ar.cabildoabierto.wiki.voteReject",
    "defs": {
      "main": {
        "type": "record",
        "key": "tid",
        "record": {
          "type": "object",
          "required": ["subject", "createdAt"],
          "properties": {
            "subject": {
              "type": "ref",
              "ref": "com.atproto.repo.strongRef"
            },
            "createdAt": {
              "type": "string",
              "format": "datetime"
            },
            "reason": {
              "type": "ref",
              "ref": "com.atproto.repo.strongRef",
              "description": "referencia a un post que justifique el voto"
            },
            "labels": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }